//bs-form
.bs-form {
    .check-list {
        text-align: center;
        margin-bottom: 10px;
        li {
            display: inline-block;
            margin-right: 5px;
            margin-bottom: 10px;
        }
    }
    .form-foot {
        text-align: center;
    }
}

@media only screen and(max-width:$mobile-breakpoint) {
    .bs-form {
        .form-wrap {
            padding: 0 15px;
        }
    }
}