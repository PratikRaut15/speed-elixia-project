.arrow_box {
background: #ffffff;
}
.arrow_box:after, .arrow_box:before {
top: 100%;
border: solid transparent;
content: " ";
height: 0;
width: 0;
position: absolute;
pointer-events: none;
}

.arrow_box:after {
border-color: rgba(255, 255, 255, 0);
border-top-color: #ffffff;
border-width: 12px;
left: 50%;
margin-left: -20px;
}
.arrow_box:before {
border-color: rgba(1, 151, 209, 0);
border-top-color: #0197d1;
border-width: 13px;
left: 50%;
margin-left: -21px;
}

