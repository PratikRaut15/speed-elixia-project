.main{
	margin : 20px;
}

.listshow{
	margin:0px;
	padding:0px;
	width : 100%;
	list-style-type: none; 
	padding: 0;
	background : #EEEEEE;
}
.listshow li{
	list-style : none;
	width : 100%;
	border-color:#CCCCCC;
	border-style:solid;
	border-width:0px 1px 1px 0px;
	text-align:left;
	font-size:12px;
}
.listshow a{
	display : block;
	color:#000;
	text-decoration : none;
	padding : 5px;
	cursor : pointer;
}
.listshow .selected{
	background : #678FD6;
	border : 1px solid #678FD6;
	color: white;
}
.listshow .selected a{
	color : #fff;
}
.selected span{
	background : #678FD6;
	color: white;
}
#loading{
	visibility:hidden;
	padding-left:5px;
}
.ajax_response, .ajax_response_2, .ajax_response_3,.ajax_response_4,.ajax_response_5,.ajax_response_6,.ajax_response_7,
.ajax_response_8,.ajax_response_9{
	position:absolute;
	top:auto;
}
.matched{
	font-weight:bold;
	color: #000000;
	background : #D8D8D8;
}
