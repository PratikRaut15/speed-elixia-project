<div  style="float:none; padding-left:41%;">
<form name="smsform" id="smsform">
    <table id="floatingpanel">
        <thead>
        <tr>
            <th id="formheader" colspan="2">Sms Tracking</th>
        </tr>
        </thead>
        <tr>
            <td colspan="2" id="nameerror" style="display: none">Please enter name</td>
            <td colspan="2" id="perfectinfo" style="display: none">Phone no. added</td>
            <td colspan="2" id="samephone" style="display: none">Phone no. already exist</td>
            <td colspan="2" id="smserror" style="display: none">Phone no. should be 10 digit</td>
            <td colspan="2" id="problem" style="display: none">Please Retry</td>        
        </tr>    
        <tr>
            <td>Name</td>
            <td><input type="text" name="name" id="name" size="30" value="" placeholder="Name"></td>
        </tr>
        <tr>
            <td>Phone No</td>
            <td><input type="text" name="phoneno" id="phoneno" size="30" value="" placeholder="Phone Number"></td>
        </tr>
        <tfoot>
        <tr>
            <td colspan="2" align="center"><input type="button" name="userdetails" class="btn  btn-primary" value="Add" onclick="checkphoneno();"></td>
        </tr>
        </tfoot>
    </table>
</form>
</div>