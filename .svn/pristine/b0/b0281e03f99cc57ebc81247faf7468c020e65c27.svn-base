  color: #fff;
  font-weight: bold;
}

select {
box-shadow:2px 2px 2px #666; 
max-width: 200px !important;
}

.panel{
  width:1224px !important;
}
.paneltitle{
   width:1208px !important;
}
input[type="file"] {
   vertical-align: middle;
   padding: 0 10px 0 10px;
}

.add_form_field
{
    background-color: #4285F4;
    border: none;
    color: white;
    padding: 8px 32px;
    text-align: center;
    text-decoration: none;
    display: inline-block;
    font-size: 16px;
    margin: 4px 2px;
    cursor: pointer;
    border:1px solid #186dad;
    border-radius: 2px;
}

.delete{
    background-color: #DB4437;
    border: none;
    color: white;
    padding: 5px 15px;
    text-align: center;
    text-decoration: none;
    display: inline-block;
    font-size: 14px;
    margin: 4px 2px;
    cursor: pointer;
    border-radius: 2px;

}